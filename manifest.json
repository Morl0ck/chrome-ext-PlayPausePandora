{
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "action-play.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.pandora.com/*"],
      "js": ["jquery.min.js", "pppandora.js"]
    }
  ],
  "description": "Play / Pause Pandora Tab",
  "icons": {
    "128": "icon_128.png"
  },
  "name": "Play / Pause Pandora Tab",
  "permissions": [
    "notifications",
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.pandora.com/*"
   ],
  "version": "1.0.4",
  "manifest_version": 2
}
